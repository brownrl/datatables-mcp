{
    "name": "brownrl/datatables-mcp",
    "description": "MCP server providing DataTables.net documentation search via stdio",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "brownrl"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "ext-json": "*",
        "symfony/dom-crawler": "^6.0|^7.0",
        "symfony/css-selector": "^6.0|^7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "DataTablesMcp\\": "src/"
        }
    },
    "bin": [
        "bin/datatables-mcp"
    ],
    "scripts": {
        "index": "php bin/datatables-mcp index",
        "serve": "php bin/datatables-mcp serve"
    }
}
